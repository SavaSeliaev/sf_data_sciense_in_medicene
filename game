import numpy as np
number = np.random.randint(1, 101)

count = 0 

while True:
    count += 1
    predict_numb = int(input('угадай число '))
    if predict_numb > number:
        print('число должно быть меньше!')
    elif predict_numb < number:
        print('число должно быть больше!')
    else:
        print(f'Молодец, это число {number}, ты потратил {count} попыток))))')
        break
